<Window x:Class="QueryLoader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QueryLoader"
        mc:Ignorable="d"
        Title="Query Loader" Height="235.887" Width="473.387">
    <Grid Margin="0,0,2,2">
        <Button x:Name="LoadButton" Content="Załaduj" HorizontalAlignment="Left" Margin="298,158,0,0" VerticalAlignment="Top" Width="75" Click="LoadButton_Click"/>
        <Button x:Name="CloseButton" Content="Zamknij" HorizontalAlignment="Left" Margin="378,158,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.227,1.5" Click="CloseButton_Click"/>
        <TextBox x:Name="FileNameText" HorizontalAlignment="Left" Height="23" Margin="10,48,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="363"/>
        <Button x:Name="FileNameButton" Content="Plik" HorizontalAlignment="Left" Margin="378,51,0,0" VerticalAlignment="Top" Width="75" Click="FileNameButton_Click"/>
        <Label x:Name="ResultLabel" Content="" HorizontalAlignment="Left" Margin="10,99,0,0" VerticalAlignment="Top" Width="443"/>
        <ProgressBar x:Name="LoadProgress" HorizontalAlignment="Left" Height="18" Margin="10,76,0,0" VerticalAlignment="Top" Width="443"/>

    </Grid>
</Window>
