%header%

GRAMMARTYPE = "LL"

DESCRIPTION = "A test grammar for error recovery in the tokenizer."

AUTHOR      = "Per Cederberg, <per at percederberg dot net>"
VERSION     = "1.0"
DATE        = "23 May 2003"

%tokens%

