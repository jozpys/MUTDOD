<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:OdraIDE.Results"
    x:Class="OdraIDE.Results.StringResultsPadView">

    <DataTemplate DataType="{x:Type local:StringResultsPad}">
        <Grid>
            <ScrollViewer>
                <!--ContentControl Grid.Row="0" Content="{Binding Path=OutputTextBlock}"/-->
                <TextBlock Text="{Binding Path=Result}" />
            </ScrollViewer>
        </Grid>
    </DataTemplate>

</ResourceDictionary>
